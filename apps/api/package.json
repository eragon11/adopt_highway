        "tsconfig-paths": "^3.12.0",
        "typeorm": "^0.2.41",
        "typescript": "^4.5.4",
        "webpack": "^5.65.0"
    },
    "jest": {
        "roots": [
            "<rootDir>"
        ],
        "modulePaths": [
            "<rootDir>"
        ],
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "./",
        "setupFilesAfterEnv": [
            "<rootDir>/jestsetup.ts"
        ],
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "jest-junit": {
        "suiteNameTemplate": "{filepath}",
        "outputDirectory": "./testresults",
        "outputName": "junit.xml"
    }
}
